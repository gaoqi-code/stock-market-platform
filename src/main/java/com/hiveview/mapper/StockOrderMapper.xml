    buy_going, update_time, add_time, close_position_time, change_quantity, revenue_model_code, 
    order_status, revenue_amount
  </sql>

  <select id="selectByPrimaryKey" resultMap="BaseResultMap"  >
    select 
    <include refid="Base_Column_List" />
    from j_stock_orders
    where id = #{id}
  </select>


